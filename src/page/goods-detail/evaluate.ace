<ul class="evaluate-list">
  {{#evaluate}}
  <li class="evaluate-item">
    <div class="evaluate-avatar">
      <img src="{{image}}" alt="">
    </div>
    <p class="evaluate-text">
      {{evaluation}}
    </p>
    <div class="evaluate-user">
      <span>{{account}}</span>
      <span>{{addTime}}</span>
    </div>
  </li>
  {{/evaluate}}
</ul>
